{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@cv": [
        "./cv.json"
      ],
      "@icons/*": [
        "src/icons/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "include": [
    "global.d.ts",
    "src",
    "src/**/*.astro",
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "src/lib"
  ]
}